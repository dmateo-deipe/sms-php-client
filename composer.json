{
  "name": "dmateo-deipe/sms-php-client",
  "description": "Fork del cliente PHP para el env√≠o de SMS con Altiria",
  "type": "library",
  "keywords": [
    "php",
    "composer",
    "sms",
    "httpclient",
    "psr-4",
    "sms-api",
    "sms-messages",
    "sms-client",
    "sms-notifications",
    "sms-gateway",
    "smsapi",
    "sms-php",
    "altiria",
    "enviar-sms",
    "envio-de-sms",
    "envio-sms",
    "sms-transaccional",
    "sms-pasarela"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Altiria",
      "email": "soporte@altiria.com",
      "homepage": "https://www.altiria.com/"
    }
  ],
  "require": {
    "php": "^8.4.0",
    "guzzlehttp/guzzle": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "AltiriaSmsPhpClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AltiriaSmsPhpClient\\Tests\\": "test/"
    }
  }
